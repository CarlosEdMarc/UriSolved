package uri;
import java.util.Arrays;
import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	
		Scanner tc = new Scanner(System.in);
		
		int[] arr = new int [3];
		int a, b, c;
		
		a = tc.nextInt();
		b = tc.nextInt();
		c = tc.nextInt();
		
        
        arr[0] = a;
        arr[1] = b;
        arr[2] = c;
 
       Arrays.sort(arr);
       for (int num:arr) {
    	   
    	   System.out.println(num); 
       }
       System.out.println("");
       System.out.println(a);
       System.out.println(b);
       System.out.println(c);
	}		
}
	

